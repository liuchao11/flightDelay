<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         id="WebApp_ID" version="3.1">
    <display-name>flightDelay</display-name>
    <welcome-file-list>
        <welcome-file>/map2.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>tempRoutesServlet</servlet-name>
        <servlet-class>servlet.tempRoutesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>tempRoutesServlet</servlet-name>
        <url-pattern>/tempRoutesServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>planeServlet</servlet-name>
        <servlet-class>servlet.planeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>planeServlet</servlet-name>
        <url-pattern>/planeServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>FlightServlet</servlet-name>
        <servlet-class>servlet.FlightServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FlightServlet</servlet-name>
        <url-pattern>/servlet/FlightServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DelayWarningServlet</servlet-name>
        <servlet-class>servlet.delayWarnServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DelayWarningServlet</servlet-name>
        <url-pattern>/servlet/delayWarnServlet</url-pattern>
    </servlet-mapping>
    <listener>
        <listener-class>thread.MyListener</listener-class>
    </listener>
</web-app>
